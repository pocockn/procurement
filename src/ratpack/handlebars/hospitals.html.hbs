<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>All Hospitals</title>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
<div class="container">
    <h1>All Games</h1>
    <div class="row">
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
            <div class="logo-header-left"><img src="http://placehold.it/240x140"></div>
        </div>
        <div class="col-xs-12 col-sm-6 col-md-8 col-lg-9">
            <div class="categories-header-right"><img src="http://placehold.it/500x140"></div>
        </div>
    </div>
    <hr>
    <div class="row games-list">
        {{#each model.created}}
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="video-container">
                            <iframe width="420" height="315" src="https://www.youtube.com/embed/xxxxxx" frameborder="0" allowfullscreen></iframe>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12">
                        <ul class="video-group">
                            <li class="video-title">{{title}}</li>
                            <li class="video-genre">{{description}}</li>
                            {{#if published}}
                                <li class="label label-primary">Published</li>
                            {{/if}}
                        </ul>
                    </div>
                </div>
            </div><!-- END COL -->
        {{/each}}
    </div><!-- END ROW -->


</div>
</body>
</html>